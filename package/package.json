{
  "name": "@gfazioli/mantine-select-stepper",
  "version": "0.9.1",
  "description": "Mantine SelectStepper is a React component that allows users to navigate through a list of options using increment and decrement buttons, providing an intuitive alternative to traditional dropdown selects for cycling through predefined values.",
  "homepage": "https://gfazioli.github.io/mantine-select-stepper/",
  "packageManager": "yarn@4.0.1",
  "license": "MIT",
  "author": "Giovambattista Fazioli <giovambattista.fazioli@gmail.com>",
  "keywords": [
    "select-stepper",
    "extension",
    "mantine",
    "react",
    "reactjs",
    "react-component",
    "nextjs",
    "ui",
    "component",
    "library",
    "typescript"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./styles.layer.css": "./dist/styles.layer.css"
  },
  "repository": "https://github.com/gfazioli/mantine-select-stepper.git",
  "peerDependencies": {
    "@mantine/core": ">=7.0.0",
    "@mantine/hooks": ">=7.0.0",
    "@tabler/icons-react": "^3.34.0",
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x"
  },
  "bugs": "https://github.com/gfazioli/mantine-select-stepper/issues"
}
